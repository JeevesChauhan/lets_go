<h1>New Poke Event</h1>

<%= form_for(@poke_event) do |f| %>
  <%= f.label :Title %>:<br>
  <%= f.text_field :title %><br>
  <%= f.label :description %>:<br>
  <%= f.text_area :description %><br>
  <%= f.hidden_field :user_id %>
  <%= f.hidden_field :land_mark_id %>
  <%= f.submit 'Submit' %>
<% end %>




<%= link_to 'Back', root_path %>
