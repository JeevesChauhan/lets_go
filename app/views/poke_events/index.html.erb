<p id="notice"><%= notice %></p>

<h1>Poke Events</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Land mark</th>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poke_events.each do |poke_event| %>
      <tr>
        <td><%= poke_event.user.trainer %></td>
        <td><%= poke_event.land_mark.name %></td>
        <td><%= poke_event.title %></td>
        <td><%= poke_event.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poke Event', new_poke_event_path %>
